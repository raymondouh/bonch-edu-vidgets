<div class="cource-group-members">
    <div class="cgm-title">
        Группа
    </div>
    <div class="cgm-group-select">
        <div class="cgm-group-line">
            <div class="cgm-group-logo">
                <img src="{{alpha}}" />
            </div>
            <div class="cgm-group-name">
                Группа "Альфа"
            </div>
            <div class="cgm-group-selector">
                <svg width="21" height="14" viewBox="0 0 21 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M2 2L10.5 11L19 2" stroke="#C0C7BA" stroke-width="3" />
                </svg>
            </div>
        </div>
    </div>
    <div class="cgm-division-line"></div>
    <div class="cgm-filter-group-students">
        <div class="cgm-filter-line">
            <div class="cgm-filter-property">
                по успеваемости
            </div>
            <div class="cgm-filter-selector">
                <svg width="15" height="10" viewBox="0 0 15 10" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M1 1L7.5 8L14 0.999999" stroke="#C0C7BA" stroke-width="2" />
                </svg>
            </div>
        </div>
    </div>
    <div class="cgm-student-list">
        {{#students}}
            <a href="{{userprofile}}">
                <div class="cgm-student">
                    <div class="cgm-student-image">
                        <img src="{{picturesrc}}" />
                    </div>
                    <div class="cgm-student-name">
                        {{fullname}}
                    </div>
                    <div class="cgm-student-selector">
                        <svg width="23" height="16" viewBox="0 0 23 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path
                                d="M22.7071 8.70711C23.0976 8.31658 23.0976 7.68342 22.7071 7.2929L16.3431 0.928934C15.9526 0.538409 15.3195 0.538409 14.9289 0.928933C14.5384 1.31946 14.5384 1.95262 14.9289 2.34315L20.5858 8L14.9289 13.6569C14.5384 14.0474 14.5384 14.6805 14.9289 15.0711C15.3195 15.4616 15.9526 15.4616 16.3431 15.0711L22.7071 8.70711ZM-8.74228e-08 9L22 9L22 7L8.74228e-08 7L-8.74228e-08 9Z"
                                />
                        </svg>
                    </div>
                </div>
            </a>
        {{/students}}
    </div>
</div>
